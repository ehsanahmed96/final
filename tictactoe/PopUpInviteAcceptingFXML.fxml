<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #F27B7A;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="incomingInviteTxt" layoutX="160.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wants To Play With You?">
         <font>
            <Font name="Bauhaus 93" size="24.0" />
         </font>
      </Text>
      <Button fx:id="acceptBtn" layoutX="304.0" layoutY="318.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" style="-fx-background-color: #ffffff;" text="Accept" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="20.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="cancelBtn" layoutX="98.0" layoutY="318.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" style="-fx-background-color: #ffffff;" text="Cancel" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="20.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Text fx:id="userNameValueTxt" layoutX="81.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ahmed">
         <font>
            <Font name="Bauhaus 93" size="24.0" />
         </font>
      </Text>
   </children>
   <effect>
      <Glow />
   </effect>
</AnchorPane>
