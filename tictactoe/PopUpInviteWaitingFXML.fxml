<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #F27B7A;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="waitingForResponseTxt" layoutX="96.0" layoutY="153.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Waiting For The Response...">
         <font>
            <Font name="Bauhaus 93" size="24.0" />
         </font>
      </Text>
      <Button fx:id="startBtn" disable="true" layoutX="304.0" layoutY="318.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" style="-fx-background-color: #ffffff;" text="Start" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="20.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="cancelBtn" layoutX="98.0" layoutY="318.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" style="-fx-background-color: #ffffff;" text="Cancel" textAlignment="CENTER">
         <font>
            <Font name="Bauhaus 93" size="20.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <ProgressIndicator fx:id="progressIndicator" layoutX="203.0" layoutY="202.0" prefHeight="83.0" prefWidth="111.0" />
